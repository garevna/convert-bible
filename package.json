{
  "name": "parse-bible-text-to-json",
  "version": "1.0.0",
  "license": "MIT",
  "description": "This package parse text file and compile json file.",
  "main": "./public",
  "files": [
    "dist/*",
    "public/*",
    "README.md"
  ],
  "scripts": {
    "clean": "rd /s /q public .cache>nul 2>&1|echo.>nul",
    "dev": "clean & webpack --mode development -w",
    "prod": "webpack --mode production",
    "package-dev": "webpack --config webpack.config.package.js --mode development -w",
    "package": "webpack --config webpack.config.package.js --mode production",
    "clear-history": "git fetch --depth=1 & git reflog expire --expire-unreachable=now --all & git gc --aggressive --prune=all"
  },
  "keywords": [
    "bible",
    "text",
    "json",
    "convert",
    "parse"
  ],
  "author": "garevna <irina.h.fylyppova@gmail.com>",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  }
}
